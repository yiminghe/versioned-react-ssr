{
  "name": "versioned-react-ssr",
  "version": "0.1.3",
  "description": "server side render react component by react version",
  "author": "yiminghe@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/yiminghe/versioned-react-ssr.git"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node",
        {
          "sourcemap": false
        }
      ],
      [
        "@pika/plugin-copy-assets",
        {
          "files": [
            "react-version"
          ]
        }
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.0",
    "@pika/plugin-copy-assets": "^0.6.0",
    "@pika/plugin-standard-pkg": "^0.6.0",
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.8.0",
    "coveralls": "^2.13.1",
    "fs-extra": "^8.1.0",
    "jest": "^24.8.0",
    "lint-staged": "^7.2.0",
    "np": "^5.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "@pika/plugin-build-types": "^0.6.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"{src,__tests__}/**/*.{js,ts}\"",
    "pub": "npm run build && np --no-publish --no-release-draft --no-cleanup &&  npm publish pkg",
    "build": "pack build && node scripts/clean.js",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "object-assign": "^4.1.1"
  }
}
